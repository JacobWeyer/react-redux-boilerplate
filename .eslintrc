{
  "name": "Fusioncorp React Boilerplate ESlint",
  "version": "0.0.1",
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "no-unused-expressions": ["error", { "allowTernary": true }]
  }
}